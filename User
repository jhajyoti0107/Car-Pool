public class User {
    int user_id;
    String name;
    String email;
    String password;

    public User(int user_id, String name, String email, String password) {
        this.user_id = user_id;
        this.name = name;
        this.email = email;
        this.password = password;
    }

    public int getUserId() {
        return user_id;
    }

    public String getName() {
        return name;
    }
}